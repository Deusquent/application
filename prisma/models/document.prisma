model Document {
  id          Int       @id @default(autoincrement())
  name        String
  type        String
  description String?
  path        String
  expiryDate  DateTime?
  createdAt   DateTime  @default(now())
  userId      Int
  user        User      @relation(fields: [userId], references: [id])
  vehicleId   Int?
  vehicle     Vehicle?  @relation(fields: [vehicleId], references: [id])
}